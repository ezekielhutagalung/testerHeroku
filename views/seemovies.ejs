  <table border="3">
        <tr>
              <th>Movie</th>
              <th>Released year</th>
              <th>Age when Released</th>
              <th>Role</th>


        </tr>
       <%data.Movies.forEach(el=>{ %>        
            <tr>
                <td><%=el.name %></td>
                <td><%=el.released_year %></td>
